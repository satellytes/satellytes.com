{
  "name": "satellytes-com",
  "private": true,
  "description": "Satellytes Website",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:satellytes/satellytes.com-new.git"
  },
  "scripts": {
    "build": "gatsby build",
    "build:gh-pages": "gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test:types": "tsc --noEmit",
    "test:lint": "eslint \"**/*.{tsx,ts}\"",
    "test:format": "prettier \"**/*.{tsx,ts,md}\" --check"
  },
  "dependencies": {
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "gatsby": "2.19.34",
    "gatsby-image": "2.2.43",
    "gatsby-plugin-emotion": "4.1.24",
    "gatsby-plugin-manifest": "2.2.45",
    "gatsby-plugin-react-helmet": "3.1.23",
    "gatsby-plugin-sharp": "2.4.8",
    "gatsby-plugin-typescript": "2.2.2",
    "gatsby-source-filesystem": "2.1.51",
    "gatsby-transformer-sharp": "2.3.17",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-helmet": "5.2.1"
  },
  "devDependencies": {
    "@types/node": "13.9.0",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/react-helmet": "5.0.15",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "prettier": "1.19.1",
    "typescript": "3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
